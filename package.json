{
  "name": "capejs",
  "version": "0.10.1",
  "description": "A lightweight Javascript UI library based on virtual-dom",
  "main": "dist/cape.js",
  "browser": "dist/cape.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "gulp test",
    "test2": "./node_modules/.bin/mocha test/runner.js -R spec",
    "build-vdom": "browserify --standalone virtual-dom node_modules/virtual-dom/index.js > vendor/virtual-dom/virtual-dom.js",
    "build-inflector": "browserify --standalone Inflector node_modules/inflected/index.js > vendor/inflected/inflected.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oiax/capejs.git"
  },
  "author": "Oiax, Inc. and other contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oiax/capejs/issues"
  },
  "homepage": "https://github.com/oiax/capejs",
  "devDependencies": {
    "eslint": "^0.15.1",
    "expect.js": "^0.3.1",
    "glob": "^5.0.0",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-karma": "0.0.4",
    "gulp-watch": "^4.2.3",
    "jsdom": "^4.0.2",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^0.1.10",
    "mocha": "^2.2.0",
    "sinon": "^1.14.1",
    "uglify-js": "latest",
    "virtual-dom": "2.0.1"
  }
}
